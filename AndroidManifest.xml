<?xml version='1.0'?>
<manifest xmlns:a='http://schemas.android.com/apk/res/android' package='com.jcomeau.studytimer' a:versionCode='0' a:versionName='0'>
    <uses-permission a:name="android.permission.WAKE_LOCK"></uses-permission>
    <receiver a:process=":remote" a:name=".Alarm"></receiver>
    <application a:label='@string/app_name'>
        <activity a:name='com.jcomeau.studytimer.MainActivity'>
             <intent-filter>
                <category a:name='android.intent.category.LAUNCHER'/>
                <action a:name='android.intent.action.MAIN'/>
             </intent-filter>
        </activity>
    </application>
</manifest>
